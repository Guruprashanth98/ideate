{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint:js": "eslint --ext .ts,.vue --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:prettier",
    "lint:js:fix": "eslint --ext .ts,.vue --ignore-path .gitignore . --fix",
    "lintfix": "prettier --write --list-different . && npm run lint:js:fix",
    "prepare": "husky install",
    "precommit:lint": "prettier --write --list-different . && npm run lint:js:fix",
    "update-types": "npx supabase gen types typescript --linked > types/supabase.ts"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxtjs/color-mode": "^3.3.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/google-fonts": "^3.0.2",
    "@nuxtjs/hanko": "^0.4.0",
    "@nuxtjs/supabase": "^1.1.2",
    "@tailwindcss/typography": "^0.5.10",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/markdown-it": "^13.0.4",
    "@types/markdown-it-attrs": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.1",
    "nuxt": "^3.7.4",
    "nuxt-icon": "^0.5.0",
    "prettier": "^3.0.3",
    "supabase": "^1.106.1",
    "typescript": "^5.2.2",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.8.0",
    "@nuxt/content": "^2.8.5",
    "@nuxtjs/tailwindcss": "^6.8.0",
    "@radix-icons/vue": "^1.0.0",
    "@types/markdown-it-highlightjs": "^3.3.3",
    "@vueuse/components": "^10.5.0",
    "@vueuse/core": "^10.5.0",
    "@vueuse/nuxt": "^10.5.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "jose": "^4.15.4",
    "lucide-vue-next": "^0.286.0",
    "markdown-it": "^13.0.2",
    "markdown-it-attrs": "^4.1.6",
    "markdown-it-highlightjs": "^4.0.1",
    "radix-vue": "^0.4.1",
    "shadcn-vue": "latest",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "module": "index.ts",
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "lint-staged": {
    "*.+(js|json|ts|vue)": [
      "npm run lintfix"
    ]
  }
}
